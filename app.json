      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.teedup.app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "teed-up",
    "extra": {
      "eas": {
        "projectId": "your-project-id"
      }
    },
    "sdkVersion": "53.0.0"
  }
} 